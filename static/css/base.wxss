/*同意协议按钮*/
.agreement-bar{
	display: flex;
	line-height: 30rpx;
	height:30rpx;
	margin:50rpx 0 60rpx 0;
}
.agreement-bar .checkbox{
	width:30rpx;
	margin-right:5px;
}
.agreement-bar .checkbox image{
	width: 100%;
}
.agreement-bar .con-text{
	display: flex;
	flex-wrap: wrap; 
	font-size:24rpx;
	color:#676767;
}
.agreement-bar text{
	color:#919191;
}
.agreement-bar navigator{	
	margin:0 3rpx; 
	color:#000;
	border-bottom:2rpx solid #000;
}
