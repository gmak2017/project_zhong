/*页面框架*/
.page{ 
	width:100%;
	height:100%;
	max-width: 800rpx;
	min-height: 100%; 
	margin:0 auto; 	  
	background: #fff;	
}
.contain{
	background: #FFFFFF; 
}
.layout{
	width:100%; 
	max-width: 800rpx;
	margin:0 auto;
}

 
.foot{
	height:100rpx;
}
.foot .fix-wrap{
	height:100rpx;
	/* bottom:68rpx; */
	/* bottom:0; */
}
.iphonex-bar{
	height:68rpx;
}
.iphonex-bar .fix-wrap{
	z-index: 1010;
	bottom:0;
	height:68rpx;
	background: #fff;
}

.empty-page{
	display: flex;
	justify-content: center;
	padding:100rpx 0;
	background: #eee;
}
.empty-page .wrap{
	width:70%;
	text-align: center;
}
.empty-page image{
	margin-bottom:30rpx;
	width:80%;
}
.empty-page .msg{
	color:#999;
	font-size:30rpx;
}
/*页面框架 end*/

/*按钮*/
.lay-btn{
	background: #000;
	text-align: center;
	height: 100rpx;
	line-height: 100rpx;
	color:#fff;
	font-size:28rpx;
}
.add-btn{
	border:1rpx solid #000;
	line-height: 98rpx;
	text-align: center;
	color:#000;
	font-size:28rpx;
}


/*按钮 end*/

/*商品列表*/
.good-list-wrap{
	display: flex;
	flex-wrap: wrap;
	padding:0 3%;
}
.goods-item{
	width:48%;
	margin: 0 1% 72rpx 1%;
}
.goods-item .pic{
	margin-bottom:30rpx;
}
.goods-item .pic image{
	width:100%;
}
.goods-item .title{
	margin-bottom:10rpx;
	font-size:30rpx;
}
.goods-item .price{
	font-size:24rpx;
}
/*商品列表 end*/

.mod-title{
	text-align: center;
	margin:0 0 50rpx 0;
}
.mod-title .en{
	margin-bottom:10rpx;
	font-size:40rpx; 	 
}
.mod-title .cn{
	font-size:34rpx;
	letter-spacing: 10rpx;
}

/**地址**/
.address-wrap{
	margin-bottom:110rpx;
}
.address-item{
	padding:40rpx 56rpx;
	border-bottom:2rpx solid #e5e5e5;
}
.address-item .name{
	line-height: 48rpx;
	font-size: 26rpx;
}
.address-item .address{
	line-height: 48rpx;
	color:#999;
	font-size:26rpx;
}

.addres-area{
	display: flex;
	align-items: center;
	justify-content: space-between;
	border-bottom:1rpx solid #f4f4f4;
	padding:40rpx 20rpx;
}
.addres-area .address-info{
	flex-grow: 2;
	max-width:70%;
	line-height: 48rpx;	
}
.addres-area .address-info .name{
	color:#333;
	font-size:26rpx;
}
.addres-area .address-info .address{
	font-size:26rpx;
	color:#989898;
}
.addres-area .iconfont{
	width:50rpx;
	text-align: center;
	color:#ccc
}

/*地址 end*/

/*物流*/
.logistics-area{
	display: flex;
	justify-content: space-between;
	/* align-items: center; */
	padding: 30rpx 20rpx;
	border-bottom:1rpx solid #f4f4f4;
}
.logistics-area .logistics-info{
	flex-grow: 2;
	max-width: 70%;
	font-size: 28rpx;
}
.logistics-area .logistics-info .title{
	margin-bottom:10rpx;
	font-size: 28rpx;
}
.logistics-area .logistics-info .day{
	margin-bottom:10rpx;
	color:#4f4f4f;
}
.logistics-area .logistics-info .price{
	
}
.logistics-area .iconfont{
	width:50rpx;
	text-align: center;
	color:#ccc
}

/*物流 end*/

/*分类tab*/
.cate-tab{
	height:64rpx;
	padding-top:20rpx;
	background: #fff;
	display: flex;
}
.cate-tab .item{
	height:64rpx;
	flex-grow: 1;
	text-align: center;
	font-size:28rpx;
	color:#666;
}
.cate-tab .item .menu{
	margin-bottom:20rpx;
}
.cate-tab .item .line{
	width:44rpx;
	height:10rpx;
	margin:0 auto;
	background: #000;
	display: none;
}
.cate-tab .active .menu{
	color:#000;
}
.cate-tab .active .line{
	display: block;
}

.cate-tab-type-2{
	display: flex;	
	height:40rpx; 
	padding:30rpx 20rpx;
}
.cate-tab-type-2 .item{
	flex-grow: 1;	
	text-align: center;
}
.cate-tab-type-2 .item .menu{
	display: inline-block;
	height:30rpx;
	line-height: 30rpx;
	position: relative; 
	color:#999;
}
.cate-tab-type-2 .item .menu .text{
	position: relative;
	font-size:28rpx;
	z-index:1;
}
.cate-tab-type-2 .active .menu .text{
	font-size:32rpx;
	color:#000;
	font-weight: bold;
}
.cate-tab-type-2 .item .menu .line{
	position: absolute;
	bottom: 0; 
	height:8rpx;
	width:100%;
	background: #fff;
	display: none;
}
.cate-tab-type-2 .active .menu .line{
	display: block;
}

/*分类tab end*/

/*目录列表*/
.cate-wrap{}
.cate-list-item{
	padding:20rpx 30rpx;
} 
.cate-list-item .cate-am{
	display: flex;
	justify-content: space-between;
	align-items: center;
}
.cate-list-item .cate-am .cate-name{
	font-size:28rpx;
}
.cate-list-item .cate-am .cate-name .en{
	margin-right:30rpx;
}
.cate-list-item .cate-am .cate-name .cn{
	margin-left:10rpx;
}
.cate-list-item .cate-am .iconfont{
	font-size:32rpx;
	color:#a9a9a9;
}

/*目录列表 end*/

/*订单列表*/
.order-wrap{}
.order-item{
	
}
.order-item .top{
	display: flex;
	justify-content: space-between;
	align-items: center;
	padding:40rpx 20rpx;
	border-bottom:1rpx solid #f4f4f4;
}
.order-item .top .label-status{
	font-size:26rpx;
	font-weight: bold;
	color:#a7322b;
}
.order-item .top .time{
	font-size:26rpx; 
	color:#b7b7b7;
}
.order-item .bottom{
	border-top:1rpx solid #f4f4f4;
	display: flex;
	justify-content: space-between;
	align-items: center;
	padding:30rpx 20rpx;
}
.order-item .bottom .total-price{
	color:#888;
	font-size:26rpx;
}
.order-item .bottom .btn-area{
	display: flex;
}
.order-item .bottom .btn-area .btn{
	height:52rpx;
	line-height: 52rpx;
	margin-left:20rpx;
	text-align: center;
	padding:0 20rpx;
	border:1rpx solid #c2c2c2;
	border-radius: 5rpx;
	color:#c2c2c2;
	font-size: 26rpx;
}
.order-item .bottom .btn-area .confirm{
	color:#a8312b;
	border:1rpx solid #a8312b;
}
/*订单列表 end*/


/*商品列表*/ 
.goods{
	display: flex;
	justify-content: space-between;
	padding:30rpx 20rpx;
}
.goods .pic{
	width:30%;
}
.goods .pic image{
	width:100%;
}
.goods .info{
	width:67%;
	position: relative;
}
.goods .info .title{
	margin-bottom:20rpx;
	font-size:26rpx;
}
.goods .info .spec{
	font-size:26rpx;
	color:#888;
}
.goods .info .price-bar{
	position: absolute;
	bottom: 0;
	width:100%;
	display: flex;
	justify-content: space-between;
}
.goods .info .price-bar .price{
	font-size:26rpx;
}
.goods .info .price-bar .num{
	color:#888;
	font-size:26rpx;
}

/*商品列表 end*/ 

/*选择按钮*/
.lay-btn-wrap{
	margin-top:20rpx;
	padding-left:20rpx;
